<p id="notice"><%= notice %></p>

<h1>Artist Shows</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @artist_shows.each do |artist_show| %>
      <tr>
        <td><%= artist_show.artist_id %></td>
        <td><%= artist_show.show_id %></td>
        <td><%= link_to 'Show', artist_show %></td>
        <td><%= link_to 'Edit', edit_artist_show_path(artist_show) %></td>
        <td><%= link_to 'Destroy', artist_show, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Artist Show', new_artist_show_path %>

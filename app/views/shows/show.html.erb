<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @show.name %>
</p>

<p>
  <strong>Event date:</strong>
  <%= @show.event_date %>
</p>



<table>
  <thead>
    <tr>
      <th>Set</th>
      <th>Encore</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @setlists.each do |setlist| %>
      <tr>
        <td><%= setlist.number %></td>
        <td><%= setlist.encore %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= song.name %></td>
        <td><%= link_to 'Show', song %></td>
        <td><%= link_to 'Edit', edit_song_path(song) %></td>
        <td><%= link_to 'Destroy', song, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>




<%= link_to 'Edit', edit_show_path(@show) %> |
<%= link_to 'Back', shows_path %>
